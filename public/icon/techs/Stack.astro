---
const {
  width = 24,
  height = 24,
  'aria-label': ariaLabel = 'Stack icon',
  title = ariaLabel,
  ...rest
} = Astro.props
---
<svg role="img" aria-label={ariaLabel} width={width} height={height} {...rest}
xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2">
  <title>{title}</title>
  <path d="M12 2l-8 4l8 4l8 -4l-8 -4"></path>
  <path d="M4 10l8 4l8 -4"></path>
  <path d="M4 18l8 4l8 -4"></path>
  <path d="M4 14l8 4l8 -4"></path>
</svg>
